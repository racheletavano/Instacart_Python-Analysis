    "## checking for mixed data types\n",
    "for col in df_ord.columns.tolist():\n",
    "  weird = (df_ord[[col]].applymap(type) != df_ord[[col]].iloc[0].apply(type)).any(axis = 1)\n",
    "  if len (df_ord[weird]) > 0:\n",
    "    print (col,'mixed')\n",
    "else: print(col,'consistent')"
   ]
